{
    "name": "apifish/shop-api",
    "description": "一个个人学习实践的电商网站 api 项目",
    "keywords": ["shop-api", "lumen", "redis", "dingo/api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.8",
        "vlucas/phpdotenv": "~2.2",
        "illuminate/redis": "^5.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
        "raveren/kint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Api\\": "app/api/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
